{
  "name": "react-tabs-redux",
  "version": "0.0.2",
  "description": "Simple React tabs component that can be used with or without any Flux (e.g. Redux) implementation",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel ./src -d lib",
    "build:dist": "npm run clean && npm run build:umd && npm run build:umd:min",
    "build:umd": "NODE_ENV=production webpack src/index.js dist/react-tabs.js",
    "build:umd:min": "NODE_ENV=production webpack -p src/index.js dist/react-tabs.min.js",
    "lint": "eslint src && npm run lint-examples || exit 0",
    "lint-examples": "cd examples/redux && npm run lint && cd ../plain-react && npm run lint",
    "test": "mocha --compilers js:babel-core/register --recursive"
  },
  "keywords": [
    "react",
    "tabs",
    "redux",
    "flux"
  ],
  "author": "Patrik Piskay",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.14.0",
    "mocha": "^2.3.4",
    "react": "^0.14.5",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.9"
  },
  "peerDependencies": {
    "react": "^0.14.5"
  },
  "dependencies": {
    "classnames": "^2.2.3"
  }
}
