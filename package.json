{
  "name": "react-tabs-redux",
  "version": "0.0.2",
  "description": "Simple React tabs component that can be used with or without any Flux (e.g. Redux) implementation",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel ./src -d lib",
    "build-umd": "NODE_ENV=production webpack src/index.js dist/react-tabs.js",
    "build-min": "NODE_ENV=production webpack -p src/index.js dist/react-tabs.min.js",
    "lint": "eslint lib || exit 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "tabs",
    "redux",
    "flux"
  ],
  "author": "Patrik Piskay",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.14.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "peerDependencies": {
    "react": "^0.14.5"
  }
}
